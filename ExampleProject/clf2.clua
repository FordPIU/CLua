lfunct add(a, b)
    return a+b
end

lfunct incr(a)
    return a++
end

lfunct decr(a)
    return a--
end

lfunct incrbb(a, b)
    return a+=b
end

lfunct decrbb(a, b)
    return a -= b
end

lfunct inf_incr(a)
    while true do
        a+=1
        print a
    end
end

lfunct self_multiply(a)
    return a**
end

